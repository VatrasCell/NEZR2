<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="start.StartController">
   <children>
      <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" />
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <center>
            <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="105.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="380.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="115.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="139.0" minHeight="10.0" prefHeight="65.0" vgrow="ALWAYS" />
                  <RowConstraints maxHeight="135.0" minHeight="10.0" prefHeight="75.0" vgrow="ALWAYS" />
              </rowConstraints>
               <children>
                  <Label fx:id="lbl_fragebogen" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Fragebogen XXX" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                  <Label fx:id="lbl_warning" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label" textAlignment="CENTER" textFill="#c90000" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
               </children>
            </GridPane>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#next" text="Start" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="16.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <top>
            <AnchorPane prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="17.0" prefWidth="600.0" text="Startseite" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="16.0" />
                  <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#adminLogin" text="Admin" AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="16.0" />
               </children>
            </AnchorPane>
         </top>
      </BorderPane>
   </children>
</StackPane>
